{
  "name": "sunpm-utils",
  "version": "0.1.0",
  "description": "个人 TypeScript 工具函数库",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "utils",
    "typescript",
    "helpers",
    "javascript",
    "utility-functions"
  ],
  "author": "sunpm",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sunpm/sunpm-utils.git"
  },
  "homepage": "https://github.com/sunpm/sunpm-utils#readme",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.15",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.1.1"
  }
}
